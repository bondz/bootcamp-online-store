<% include partial/head %>

	<body translate="no">
		<nav class="teal">
			<div class="nav-wrapper">
				<a href="#" class="brand-logo">
					<%= storeName %>
				</a>
				<ul id="nav-mobile" class="right hide-on-med-and-down">
					<li>
						<a href="contact"> <i class="material-icons left">phone</i> Contact</a>
					</li>
				</ul>
			</div>
		</nav>
		<div class="body">
			<h4 class="center"> AVAILABLE ITEMS </h4>
			<ul class="product-list">
				<% storeItems.forEach(function(item) { %>
					<li class="relative">
						<img class="radius" src="<%= item.productImage %>" title="<%= item.description %>" alt="<%= item.description %>" width="200"
							height="300" />
						<h2 class="product-name center">
							<a href="#product-page" title="<%= item.productName %>">
								<%= item.productName %>
							</a>
						</h2>
						<small class="new absolute">New</small>
						<a class="add-to-cart absolute radius hover" href="#" title="">$<%= item.productPrice %></a>
						<a class="share share-facebook absolute hover" href="#share-facebook" title="Share on Facebook">f</a>
						<a class="share share-twitter absolute hover" href="#share-twitter" title="Share on Twitter">t</a>
					</li>
					<% }) %>
			</ul>
		</div>
	</body>