<title> <%= title %> </title>        

<form class="" action="signup" method="POST">
        <h3> Login </h3>
        
    
        <div class="input-field ">
                <input name="email" id="email" type="email" class="validate" require>
                <label for="email">Email</label>
        </div>



        <div class="input-field ">
                <input name="password" id="password" type="password" class="validate" require>
                <label for="password">Password</label>
        </div>


        <div class="input-field">
                <button class="">
                Login
                </button>
        </div>
</form>